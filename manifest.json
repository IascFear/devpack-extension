{
  "manifest_version": 3,
  "name": "DevPack",
  "version": "1.0",
  "description": "A Chrome extension packed with resources to simplify development and boost productivity.",
  "action": {
    "default_icon": {
      "16": "Assets/logo.png",
      "64": "Assets/logo.png",
      "128": "Assets/logo.png"
    }
  },
  "side_panel": {
    "default_path": "popup.html"
  },
  "permissions": [
    "storage",
    "sidePanel",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "Assets/logo.png",
    "64": "Assets/logo.png",
    "128": "Assets/logo.png"
  }
}